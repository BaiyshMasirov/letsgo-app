@model AdminDto

<h4 class="font-weight-bold">Информация о пользователе</h4>
<div class="row mt-4">
    <div class="col-md-12 col">
        <div class="mb-3">
            <p class="text-primary mb-1 text-18">
                <i class="i-Business-ManWoman mr-1"></i>ФИО
            </p>
            <span>@Model.FullName</span>
        </div>
        <div class="mb-3">
            <p class="text-primary mb-1 text-18"><i class="i-Tag-4 mr-1"></i>Логин</p>
            <span>@Model.Email</span>
        </div>

        <div class="mb-3">
            <p class="text-primary mb-1 text-18"><i class="i-ID-Card mr-1"></i>Номер телефона</p>
            <span>@Model.PhoneNumber</span>
        </div>

        <div class="mb-3">
            <p class="text-primary mb-1 text-18">
                <i class="i-Map2 mr-1"></i>Статус
            </p>
            <span>@Model.Status</span>
        </div>
    </div>
</div>